{
  "name": "My extension",
  "version": "1.0",
  "description": "Starter sample of extension.",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/"],
      "run_at": "document_idle",
      "js": ["contentScripts.js"]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
